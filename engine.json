{
  "id": "default",
  "description": "Default Classification",
  "engineFactory": "org.template.classification.ClassificationEngine",
  "datasource": {
    "params": {
      "appName": "classi"
    }
  },
  "algorithms": [
    {
      "name": "naive",
      "params": {
        "lambda": 1.0
      }
    },
    {
      "name": "randomforest",
      "params": {
        "numClasses": 7,
        "numTrees": 12,
        "featureSubsetStrategy": "auto",
        "impurity": "gini",
        "maxDepth": 4,
        "maxBins": 100
      }
    }
  ]
}